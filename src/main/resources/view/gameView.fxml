<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.view.GameViewController">
    <children>
      <HBox layoutX="1008.0" prefHeight="500.0" prefWidth="272.0">
         <children>
                <VBox prefHeight="500.0" prefWidth="299.0">
                    <children>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="253.0" prefWidth="228.0">
                     <children>
                                    <TextArea fx:id="chatWindow" accessibleRole="TEXT" editable="false" layoutX="6.0" layoutY="17.0" prefHeight="226.0" prefWidth="250.0" />
                     </children>
                          </AnchorPane>
                          <HBox prefHeight="37.0" prefWidth="212.0">
                              <children>
                        <TextField fx:id="chatTextField" onAction="#sendChatMessage" prefHeight="27.0" prefWidth="147.0">
                           <padding>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </padding>
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </HBox.margin>
                        </TextField>
                                  <Button mnemonicParsing="false" onAction="#sendChatMessage" prefHeight="27.0" prefWidth="63.0" text="Submit">
                                      <HBox.margin>
                                          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                      </HBox.margin>
                                      <font>
                                          <Font size="12.0" />
                                      </font>
                                  </Button>
                              </children>
                          </HBox>
                    </children>
                </VBox>
         </children>
      </HBox>
    </children>
</AnchorPane>
