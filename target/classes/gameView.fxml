<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="900.0" prefWidth="1600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.view.GameViewController">
    <children>
        <HBox fx:id="Background" alignment="TOP_RIGHT" layoutX="-53.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="800.0" prefWidth="1000.0">
            <children>
                <VBox prefHeight="529.0" prefWidth="272.0">
                    <children>
                        <VBox prefHeight="312.0" prefWidth="269.0">
                            <children>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="253.0" prefWidth="257.0">
                           <children>
                                          <TextArea fx:id="chatWindow" accessibleRole="TEXT" editable="false" layoutX="6.0" layoutY="8.0" prefHeight="225.0" prefWidth="255.0" />
                           </children>
                                </AnchorPane>
                                <HBox prefHeight="33.0" prefWidth="269.0">
                                    <children>
                              <TextField fx:id="chatTextArea" onAction="#sendChatMessage" prefHeight="33.0" prefWidth="200.0">
                                 <padding>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </padding>
                                 <HBox.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </HBox.margin></TextField>
                                        <Button fx:id="submitButton" mnemonicParsing="false" onAction="#sendChatMessage" prefHeight="27.0" prefWidth="64.0" text="Submit">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                            </HBox.margin>
                                            <font>
                                                <Font size="12.0" />
                                            </font>
                                        </Button>
                                    </children>
                                </HBox>
                            </children>
                            <VBox.margin>
                                <Insets bottom="5.0" left="5.0" />
                            </VBox.margin>
                            <padding>
                                <Insets bottom="5.0" />
                            </padding>
                        </VBox>
                    </children>
                </VBox>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
    </children>
</AnchorPane>
